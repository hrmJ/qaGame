		"c8": "^7.11.3",
		"jsdom": "^20.0.0",
		"msw": "0.31.0-beta.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
